{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "BinaryDB": "Server=(localdb)\\mssqllocaldb;Database=Binary_DataBase;Trusted_Connection=True;"
  },

  "WorkerSerializeFile": "C:\\json.txt",
  "RabbitMq": {
    "FromServerToWorker": {
      "ExchangeName": "ServerToWorkerExchanger",
      "ExchangeType": "direct",
      "QueueName": "ControllerActionNamesQueue",
      "RoutingKey": "names"
    },
    "FromWorkerToServer": {
      "ExchangeName": "WorkerToServerExchanger",
      "ExchangeType": "direct",
      "QueueName": "SerializationResultQueue",
      "RoutingKey": "results"

    }
  }
}
